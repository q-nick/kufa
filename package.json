{
  "name": "npm-gui",
  "version": "3.1.1",
  "description": "Graphical tool for managing javascript project dependencies - in a friendly way.",
  "keywords": [
    "gui",
    "npm",
    "yarn",
    "pnpm",
    "view",
    "client",
    "dependencies",
    "node_modules",
    "packages",
    "installer",
    "manager"
  ],
  "main": "index.js",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "prepack": "npm run build",
    "dev:server": "ts-node-dev --files server/development.ts",
    "dev:client": "parcel client/index.html",
    "dev": "npm-run-all -p dev:*",
    "build:server": "tsc --project server/tsconfig.json",
    "build:client": "parcel build client/index.html --no-source-maps",
    "build": "rm -rf dist && npm-run-all -p build:*",
    "lint:ts": "tsc",
    "lint:eslint": "eslint . --ignore-path=.gitignore",
    "lint:prettier": "prettier --check . --ignore-path=.gitignore",
    "lint": "npm-run-all lint:* -p",
    "test": "jest --runInBand",
    "ci:test": "env NODE_TEST=true npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/q-nick/npm-gui.git"
  },
  "author": "Paweł Stefański",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/q-nick/npm-gui/issues"
  },
  "bin": {
    "npm-gui": "index.js"
  },
  "homepage": "https://github.com/q-nick/npm-gui#readme",
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.8",
    "@types/react": "^17.0.49",
    "@types/react-dom": "^17.0.17",
    "@types/react-router-dom": "^5.3.3",
    "@types/rimraf": "^3.0.2",
    "@types/styled-components": "^5.1.26",
    "@types/supertest": "^2.0.12",
    "eslint-config-turbocharge": "^0.1.8",
    "fs-extra": "^10.1.0",
    "jest": "^28.1.3",
    "jest-extended": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "open-iconic": "^1.1.1",
    "parcel": "^2.7.0",
    "process": "^0.11.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.3",
    "styled-components": "^5.3.5",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "cross-spawn": "^7.0.3",
    "open": "^8.4.0",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 1.0.0"
  }
}
