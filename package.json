{
  "name": "npm-gui",
  "version": "4.0.0-beta",
  "description": "A graphical tool for managing javascript project dependencies.",
  "keywords": [
    "gui",
    "npm",
    "yarn",
    "pnpm",
    "view",
    "client",
    "dependencies",
    "node_modules",
    "packages",
    "installer",
    "manager"
  ],
  "main": "index.js",
  "module": "dist/module.js",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "desktop": "electron .",
    "prepack": "npm run build",
    "dev:server": "ts-node-dev --files server/development.ts",
    "dev:client": "vite",
    "dev": "npm-run-all -p dev:*",
    "build": "rm -rf dist && vite build",
    "lint:ts": "tsc",
    "lint:eslint": "eslint . --ignore-path=.gitignore",
    "lint:prettier": "prettier --check . --ignore-path=.gitignore",
    "lint": "npm-run-all lint:* -p",
    "test": "jest",
    "testt": "cucumber-js",
    "ci:test": "env NODE_TEST=true npm run test",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/q-nick/npm-gui.git"
  },
  "author": "Paweł Stefański",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/q-nick/npm-gui/issues"
  },
  "bin": {
    "npm-gui": "index.js"
  },
  "homepage": "https://npm-gui.nullapps.dev",
  "devDependencies": {
    "@cucumber/cucumber": "^9.5.1",
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/plugin-auto-unpack-natives": "^6.4.2",
    "@swc-node/register": "^1.6.7",
    "@tanstack/query-sync-storage-persister": "^4.32.6",
    "@tanstack/react-query": "^4.35.3",
    "@tanstack/react-query-persist-client": "^4.32.6",
    "@types/cors": "^2.8.14",
    "@types/fs-extra": "v9.0.13",
    "@types/jest": "^29.5.3",
    "@types/node": "v11.15.54",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@types/supertest": "^2.0.12",
    "autoprefixer": "^10.4.16",
    "electron": "^26.2.0",
    "eslint-config-turbocharge": "^1.0.0-beta-1",
    "fs-extra": "v10.1.0",
    "jest": "^29.6.2",
    "jest-extended": "^4.0.1",
    "npm-gui-tests": "^1.1.1",
    "npm-run-all": "^4.1.5",
    "open-iconic": "^1.1.1",
    "parcel": "^2.9.3",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.30",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-router-dom": "^5.3.4",
    "styled-components": "^6.0.7",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "@vitejs/plugin-react": "^4.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "prettier-plugin-gherkin": "^2.0.0",
    "vite": "^4.4.9",
    "use-between": "^1.3.5"
  },
  "targets": {
    "main": false,
    "client": {
      "source": "client/index.html",
      "context": "browser",
      "distDir": "dist"
    },
    "server": {
      "source": "server/index.ts",
      "context": "node",
      "outputFormat": "commonjs",
      "distDir": "dist"
    }
  },
  "dependencies": {
    "@trpc/client": "^10.38.4",
    "@trpc/react-query": "^10.38.4",
    "@trpc/server": "^10.38.4",
    "open": "^9.1.0",
    "zod": "^3.22.2"
  },
  "engines": {
    "node": ">= 11.0.0",
    "npm": ">= 6.0.0"
  }
}
